{
  "name": "excalibur-jasmine",
  "version": "0.2.0",
  "description": "Jasmine matchers for testing excalibur",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/excaliburjs/excalibur-jasmine/"
  },
  "scripts": {
    "build": "webpack --mode none",
    "test": "node ./node_modules/karma/bin/karma start",
    "test:watch": "node ./node_modules/karma/bin/karma start --single-run=false --browsers=Chrome_with_debug"
  },
  "keywords": [
    "Excalibur",
    "Excalibur.js",
    "testing",
    "jasmine"
  ],
  "author": "Erik Onarheim",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@types/jasmine": "3.10.0",
    "@types/webpack": "4.41.31",
    "@types/webpack-env": "1.16.3",
    "copy-webpack-plugin": "9.0.1",
    "excalibur": "0.25.0",
    "jasmine-core": "3.10.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-jasmine": "4.0.1",
    "karma-sourcemap-loader": "0.3.8",
    "karma-webpack": "5.0.0",
    "wallaby-webpack": "3.9.16",
    "puppeteer": "10.4.0",
    "ts-loader": "9.2.6",
    "typescript": "4.4.4",
    "webpack": "5.50.0",
    "webpack-cli": "4.9.1"
  },
  "dependencies": {}
}
